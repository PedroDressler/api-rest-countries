<%- contentFor('body') %>
<div class="jumbotron text-center">
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Flag</th>
                <th>Population</th>
                <th>Language</th>
            </tr>
        </thead>
        <tbody>
            <% for ( var pais in paises ) {%>
                <tr>
                    <td><% pais.name %></td>
                    <td><img src="<% pais.flag %>"></td>
                    <td>Population</td>
                    <td>Language</td>
                </tr>
            <% }%>
        </tbody>
      </table>
</div>